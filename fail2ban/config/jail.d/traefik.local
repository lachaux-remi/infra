[traefik-auth]
enabled = true
chain = DOCKER-USER
port = http,https
filter = traefik-auth
mode = aggressive
logpath = /logs/traefik/traefik.log

[traefik-auth-ddos]
filter = traefik-auth[mode=ddos]

[traefik-botsearch]
enabled = true
chain = DOCKER-USER
port = http,https
filter = traefik-botsearch
mode = aggressive
logpath = /logs/traefik/traefik.log